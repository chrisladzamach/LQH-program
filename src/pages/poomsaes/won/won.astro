---
import Layout from '../../../layouts/Layout.astro';
import ButtonMenu from '../../../components/ButtonMenu.astro';
// import TechniquesByGrade from '../components/TechniquesByGrade.astro';
import Header from '../../../components/Header.astro';
---

<Layout>  
  <section class="">
    <Header name="Won Poomsaes" href='/poomsaes' />
    <article class="flex flex-col gap-3">
      <ButtonMenu title="Won Il Dan" href="/poomsaes/won/wonIlDan" class="text-black font-bold py-2 px-2 bg-orange-600 rounded-sm focus:bg-blue-700 lg:hover:bg-blue-400 transition duration-300 ease-in-out" />
      <ButtonMenu title="Won I Dan" href="/poomsaes/won/wonIDan" class="text-black font-bold py-2 px-2 bg-blue-700 rounded-sm focus:bg-blue-700 lg:hover:bg-blue-400 transition duration-300 ease-in-out" />
      <ButtonMenu title="Won Sam Dan" href="/poomsaes/won/wonSamDan" class="text-black font-bold py-2 px-2 bg-red-700 rounded-sm focus:bg-blue-700 lg:hover:bg-blue-400 transition duration-300 ease-in-out" />
      <ButtonMenu title="Won Sa Dan" href="/poomsaes/won/wonSaDan" class="text-black font-bold py-2 px-2 bg-[#78350f] rounded-sm focus:bg-blue-700 lg:hover:bg-blue-400 transition duration-300 ease-in-out" />
      <ButtonMenu title="Won O Dan" href="/poomsaes/won/wonODan" class="text-yellow-300 font-bold py-2 px-2 bg-black rounded-sm focus:bg-blue-700 lg:hover:bg-blue-400 transition duration-300 ease-in-out" />
    </article>
  </section>
</Layout>

